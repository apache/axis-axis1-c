<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from RetailerService WSDL service -->

  <service name="RetailerPort" provider="java:RPC" style="rpc" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://www.ws-i.org/SampleApplications/SupplyChainManagement/2002-08/Retailer.wsdl"/>
      <parameter name="wsdlServiceElement" value="RetailerService"/>
      <parameter name="wsdlServicePort" value="RetailerPort"/>
      <parameter name="className" value="org.apache.axis.wsi.scm.retailer.RetailerSoapBindingImpl"/>
      <parameter name="wsdlPortType" value="RetailerPortType"/>
      <operation name="getCatalog" qname="operNS:getCatalog" xmlns:operNS="http://www.ws-i.org/SampleApplications/SupplyChainManagement/2002-08/Retailer.wsdl" returnQName="return" returnType="rtns:CatalogType" xmlns:rtns="http://www.ws-i.org/SampleApplications/SupplyChainManagement/2002-08/RetailCatalog.xsd" >
      </operation>
      <operation name="submitOrder" qname="operNS:submitOrder" xmlns:operNS="http://www.ws-i.org/SampleApplications/SupplyChainManagement/2002-08/Retailer.wsdl" returnQName="return" returnType="rtns:PartsOrderResponseType" xmlns:rtns="http://www.ws-i.org/SampleApplications/SupplyChainManagement/2002-08/RetailOrder.xsd" >
        <parameter name="PartsOrder" type="tns:PartsOrderType" xmlns:tns="http://www.ws-i.org/SampleApplications/SupplyChainManagement/2002-08/RetailOrder.xsd"/>
        <parameter name="CustomerDetails" type="tns:CustomerDetailsType" xmlns:tns="http://www.ws-i.org/SampleApplications/SupplyChainManagement/2002-08/RetailOrder.xsd"/>
        <parameter qname="pns:Configuration" xmlns:pns="http://www.ws-i.org/SampleApplications/SupplyChainManagement/2002-08/Configuration.xsd" type="tns:ConfigurationType" xmlns:tns="http://www.ws-i.org/SampleApplications/SupplyChainManagement/2002-08/Configuration.xsd" inHeader="true"/>
        <fault name="ConfigurationFault" qname="fns:ConfigurationFault" xmlns:fns="http://www.ws-i.org/SampleApplications/SupplyChainManagement/2002-08/Configuration.xsd" class="org.apache.axis.wsi.scm.configuration.ConfigurationFaultType" type="tns:ConfigurationFaultType" xmlns:tns="http://www.ws-i.org/SampleApplications/SupplyChainManagement/2002-08/Configuration.xsd"/>
        <fault name="InvalidProductCode" qname="fns:InvalidProductCode" xmlns:fns="http://www.ws-i.org/SampleApplications/SupplyChainManagement/2002-08/RetailOrder.xsd" class="org.apache.axis.wsi.scm.retailer.order.InvalidProductCodeType" type="tns:InvalidProductCodeType" xmlns:tns="http://www.ws-i.org/SampleApplications/SupplyChainManagement/2002-08/RetailOrder.xsd"/>
        <fault name="BadOrder" qname="fns:BadOrderReason" xmlns:fns="http://www.ws-i.org/SampleApplications/SupplyChainManagement/2002-08/RetailOrder.xsd" class="org.apache.axis.wsi.scm.retailer.BadOrderFault" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <parameter name="allowedMethods" value="submitOrder getCatalog"/>

      <typeMapping
        xmlns:ns="http://www.ws-i.org/SampleApplications/SupplyChainManagement/2002-08/Configuration.xsd"
        qname="ns:ConfigurationEndpointRole"
        type="java:org.apache.axis.wsi.scm.configuration.ConfigurationEndpointRole"
        serializer="org.apache.axis.encoding.ser.EnumSerializerFactory"
        deserializer="org.apache.axis.encoding.ser.EnumDeserializerFactory"
        encodingStyle=""
      />
      <typeMapping
        xmlns:ns="http://www.ws-i.org/SampleApplications/SupplyChainManagement/2002-08/Configuration.xsd"
        qname="ns:ConfigurationType"
        type="java:org.apache.axis.wsi.scm.configuration.ConfigurationType"
        serializer="org.apache.axis.encoding.ser.BeanSerializerFactory"
        deserializer="org.apache.axis.encoding.ser.BeanDeserializerFactory"
        encodingStyle=""
      />
      <typeMapping
        xmlns:ns="http://www.ws-i.org/SampleApplications/SupplyChainManagement/2002-08/RetailOrder.xsd"
        qname="ns:PartsOrderResponseType"
        type="java:org.apache.axis.wsi.scm.retailer.order.PartsOrderResponseType"
        serializer="org.apache.axis.encoding.ser.BeanSerializerFactory"
        deserializer="org.apache.axis.encoding.ser.BeanDeserializerFactory"
        encodingStyle=""
      />
      <typeMapping
        xmlns:ns="http://www.ws-i.org/SampleApplications/SupplyChainManagement/2002-08/RetailOrder.xsd"
        qname="ns:PartsOrderType"
        type="java:org.apache.axis.wsi.scm.retailer.order.PartsOrderType"
        serializer="org.apache.axis.encoding.ser.BeanSerializerFactory"
        deserializer="org.apache.axis.encoding.ser.BeanDeserializerFactory"
        encodingStyle=""
      />
      <typeMapping
        xmlns:ns="http://www.ws-i.org/SampleApplications/SupplyChainManagement/2002-08/RetailOrder.xsd"
        qname="ns:productNumber"
        type="java:org.apache.axis.wsi.scm.retailer.order.ProductNumber"
        serializer="org.apache.axis.encoding.ser.SimpleSerializerFactory"
        deserializer="org.apache.axis.encoding.ser.SimpleDeserializerFactory"
        encodingStyle=""
      />
      <typeMapping
        xmlns:ns="http://www.ws-i.org/SampleApplications/SupplyChainManagement/2002-08/RetailOrder.xsd"
        qname="ns:CustomerReferenceType"
        type="java:org.apache.axis.wsi.scm.retailer.order.CustomerReferenceType"
        serializer="org.apache.axis.encoding.ser.SimpleSerializerFactory"
        deserializer="org.apache.axis.encoding.ser.SimpleDeserializerFactory"
        encodingStyle=""
      />
      <typeMapping
        xmlns:ns="http://www.ws-i.org/SampleApplications/SupplyChainManagement/2002-08/RetailOrder.xsd"
        qname="ns:CustomerDetailsType"
        type="java:org.apache.axis.wsi.scm.retailer.order.CustomerDetailsType"
        serializer="org.apache.axis.encoding.ser.BeanSerializerFactory"
        deserializer="org.apache.axis.encoding.ser.BeanDeserializerFactory"
        encodingStyle=""
      />
      <typeMapping
        xmlns:ns="http://www.ws-i.org/SampleApplications/SupplyChainManagement/2002-08/Configuration.xsd"
        qname="ns:ConfigurationEndpointType"
        type="java:org.apache.axis.wsi.scm.configuration.ConfigurationEndpointType"
        serializer="org.apache.axis.encoding.ser.SimpleSerializerFactory"
        deserializer="org.apache.axis.encoding.ser.SimpleDeserializerFactory"
        encodingStyle=""
      />
      <typeMapping
        xmlns:ns="http://www.ws-i.org/SampleApplications/SupplyChainManagement/2002-08/RetailOrder.xsd"
        qname="ns:>InvalidProductCodeType>Reason"
        type="java:org.apache.axis.wsi.scm.retailer.order._InvalidProductCodeType_Reason"
        serializer="org.apache.axis.encoding.ser.EnumSerializerFactory"
        deserializer="org.apache.axis.encoding.ser.EnumDeserializerFactory"
        encodingStyle=""
      />
      <typeMapping
        xmlns:ns="http://www.ws-i.org/SampleApplications/SupplyChainManagement/2002-08/RetailCatalog.xsd"
        qname="ns:CatalogType"
        type="java:org.apache.axis.wsi.scm.retailer.catalog.CatalogType"
        serializer="org.apache.axis.encoding.ser.BeanSerializerFactory"
        deserializer="org.apache.axis.encoding.ser.BeanDeserializerFactory"
        encodingStyle=""
      />
      <typeMapping
        xmlns:ns="http://www.ws-i.org/SampleApplications/SupplyChainManagement/2002-08/RetailOrder.xsd"
        qname="ns:PartsOrderResponseItem"
        type="java:org.apache.axis.wsi.scm.retailer.order.PartsOrderResponseItem"
        serializer="org.apache.axis.encoding.ser.BeanSerializerFactory"
        deserializer="org.apache.axis.encoding.ser.BeanDeserializerFactory"
        encodingStyle=""
      />
      <typeMapping
        xmlns:ns="http://www.ws-i.org/SampleApplications/SupplyChainManagement/2002-08/RetailCatalog.xsd"
        qname="ns:productNumber"
