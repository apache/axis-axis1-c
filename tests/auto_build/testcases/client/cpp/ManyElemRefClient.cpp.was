#include "ManyElemRefWS.h" 
int main(int argc, char* argv[])
{
  ManyElemRefWS *ws = new ManyElemRefWS();
  ws->SetEndpointURI("http://localhost:9080/ManyElemRef/services/sampleWS");

  Element1_Array input;
  Element1_Array result;
  int i;

  Element1 types[10];


  Element1* current;


  for ( i = 0; i < 10; i++ ) {
    current = new Element1 ();
    current->item = i;
    types[i] = *current;
  }

  input.m_Array = types;
  input.m_Size = 10;

	result = ws->getInput(input);

  printf ("Success \n");
  Element1 *arrayResult = result.m_Array;
  for ( i = 0; i < 10; i++ ) {
    printf (" Result = %d : %s \n" ,
           arrayResult->item);
           arrayResult++;
  }

  return 0;
}
